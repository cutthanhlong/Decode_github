<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:name="btn_checkbox_checked"
    android:height="18dp"
    android:width="18dp"
    android:viewportWidth="18"
    android:viewportHeight="18">
    <group
        android:name="icon_null"
        android:scaleX="0.2"
        android:scaleY="0.2"
        android:translateX="9"
        android:translateY="9">
        <group
            android:name="check"
            android:scaleX="5"
            android:scaleY="5">
            <path
                android:name="check_path_merged"
                android:fillColor="?attr/colorAccent"
                android:pathData="@string/sephiroth_checkbox3_path_checked"/>
        </group>
        <group
            android:name="box_dilate"
            android:scaleX="5"
            android:scaleY="5">
            <path
                android:name="box_inner_merged"
                android:fillColor="?attr/colorAccent"
                android:pathData="@string/sephiroth_checkbox3_path_circle_centered"
                android:fillAlpha="0"/>
        </group>
    </group>
</vector>