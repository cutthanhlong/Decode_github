<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="@dimen/size_1dp"
                android:color="?attr/selected_color"/>
            <corners android:radius="8dp"/>
            <solid android:color="@color/transparent"/>
        </shape>
    </item>
    <item
        android:left="@dimen/size_1dp"
        android:top="@dimen/size_1dp"
        android:right="@dimen/size_1dp"
        android:bottom="@dimen/size_1dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="@dimen/size_1dp"
                android:color="?attr/bottombarcolor"/>
            <corners android:radius="8dp"/>
            <solid android:color="@color/transparent"/>
        </shape>
    </item>
</layer-list>