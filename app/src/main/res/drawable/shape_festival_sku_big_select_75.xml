<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:startColor="#35fedc"
                android:endColor="#35fedc"
                android:angle="0"/>
            <corners android:radius="@dimen/size_12dp"/>
        </shape>
    </item>
    <item
        android:left="@dimen/size_2dp"
        android:top="@dimen/size_2dp"
        android:right="@dimen/size_2dp"
        android:bottom="@dimen/size_2dp">
        <shape>
            <gradient
                android:startColor="#fefdf7"
                android:endColor="#fffffc"
                android:angle="270"/>
            <corners android:radius="@dimen/size_12dp"/>
        </shape>
    </item>
</layer-list>